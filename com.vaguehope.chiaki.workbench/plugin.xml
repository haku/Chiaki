<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="com.vaguehope.chiaki.workbench.Application">
         </run>
      </application>
   </extension>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="com.vaguehope.chiaki.workbench.application"
            name="chiaki">
         <property
               name="appName"
               value="chiaki">
         </property>
      </product>
   </extension>

</plugin>
